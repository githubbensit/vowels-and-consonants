letter = input("Enter a character: ")

if(letter=='A' or 
   letter=='a' or 
   letter=='E' or 
   letter =='e' or 
   letter=='I'  or 
   letter=='i' or 
   letter=='O' or 
   letter=='o' or 
   letter=='U' or 
   letter=='u'):
    print(letter, "is a Vowel")
else:
    print(letter, "is a Consonant")	
